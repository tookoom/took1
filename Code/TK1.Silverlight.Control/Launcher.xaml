<UserControl x:Class="Took1.Silverlight.Controls.Launcher"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="Auto" Height="105" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" d:DesignWidth="666" d:DesignHeight="163" xmlns:Numericon_DM_Web_Silverlight="clr-namespace:Took1.Silverlight">
    <Grid x:Name="LayoutRoot" Background="{x:Null}">
    	<Grid.RowDefinitions>
    		<RowDefinition Height="Auto" MinHeight="105"/>
    	</Grid.RowDefinitions>
    	<StackPanel HorizontalAlignment="Center" Margin="0,0,0,25" VerticalAlignment="Bottom" x:Name="stackPanelLauncherItems" Orientation="Horizontal" d:LayoutOverrides="GridBox"/>
    </Grid>
</UserControl>
